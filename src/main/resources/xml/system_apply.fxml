<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nalecy.www.project.page.SystemMasterApplyPage">
   <children>
      <TableView fx:id="pendingApply" layoutX="35.0" layoutY="37.0" prefHeight="333.0" prefWidth="305.0">
          <columns>
              <TableColumn prefWidth="75.0" text="项目名">
                  <cellValueFactory>
                      <PropertyValueFactory property="projectName" />
                  </cellValueFactory>
              </TableColumn>
              <TableColumn prefWidth="75.0" text="评委姓名">
                  <cellValueFactory>
                      <PropertyValueFactory property="userName" />
                  </cellValueFactory>
              </TableColumn>
          </columns>
      </TableView>
      <Button fx:id="nonPassBtn" layoutX="383.0" layoutY="244.0" mnemonicParsing="false" text="不通过审核" onAction="#onClickNonPass"/>
      <Button fx:id="backBtn" layoutX="401.0" layoutY="68.0" mnemonicParsing="false" text="返回" onAction="#onClickBack"/>
      <Button fx:id="passBtn" layoutX="389.0" layoutY="156.0" mnemonicParsing="false" text="通过审核" onAction="#onClickPass"/>
   </children>
</AnchorPane>
