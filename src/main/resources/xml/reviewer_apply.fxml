<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nalecy.www.project.page.ReviewerApplyPage">
   <children>
      <Label layoutX="334.0" layoutY="47.0" text="通过的审核" />
      <Label layoutX="55.0" layoutY="47.0" text="申请中的审核" />
      <TableView fx:id="pendingTable" layoutX="46.0" layoutY="69.0" prefHeight="200.0" prefWidth="200.0">
          <columns>
              <TableColumn prefWidth="75.0" text="项目名">
                  <cellValueFactory>
                      <PropertyValueFactory property="projectName" />
                  </cellValueFactory>
              </TableColumn>
              <TableColumn prefWidth="75.0" text="评委姓名">
                  <cellValueFactory>
                      <PropertyValueFactory property="userName" />
                  </cellValueFactory>
              </TableColumn>
          </columns>
      </TableView>
      <TableView fx:id="passTable" layoutX="324.0" layoutY="69.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn prefWidth="75.0" text="项目名">
                <cellValueFactory>
                    <PropertyValueFactory property="projectName" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="75.0" text="评委姓名">
                <cellValueFactory>
                    <PropertyValueFactory property="userName" />
                </cellValueFactory>
            </TableColumn>
        </columns>
      </TableView>
      <Button fx:id="reviewBtn" layoutX="397.0" layoutY="299.0" mnemonicParsing="false" text="进行审核" onAction="#onClickReview" />
   </children>
</AnchorPane>
